<Window x:Class="UrlResolve.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UrlResolve"
        mc:Ignorable="d"
        Title="MainWindow" Height="194" Width="264.5">
    <Grid HorizontalAlignment="Left" Width="254" Margin="0,0,0,6">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="316*"/>
            <ColumnDefinition Width="17*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="UrlResolveBtn" Content="处理进度" HorizontalAlignment="Left" Height="60" Margin="55,20,0,0" VerticalAlignment="Top" Width="149" Click="UrlResolve_Click"/>
        <ProgressBar x:Name="ProgressNow" HorizontalAlignment="Left" Height="26" Margin="55,98,0,0" VerticalAlignment="Top" Width="149" Value="{Binding progressOC}"/>
    </Grid>
</Window>
